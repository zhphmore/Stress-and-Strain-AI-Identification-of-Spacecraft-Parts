<root>
    <!--************************************************************-->
    <!--非常重要：part设置小件或大件-->
    <!--若<part>的值为small，将会采用<small>的值；若<part>的值为large，将会采用<large>的值-->
    <part>small</part>
    <!--<part>large</part>-->
    <!--************************************************************-->
    <!--**********小型零件**********-->
    <small>
        <path_save>C:/projects/make_dataset/files_small</path_save>
        <inp>
            <!--模板inp文件名-->
            <inp_template>Job_TMPL_small.inp</inp_template>
            <!--模板inp文件里，节点编号开始与结束的行-->
            <!--小件一共188191个结点，节点编号从第10行开始，第188200行结束-->
            <node_row_start>10</node_row_start>
            <node_row_end>188200</node_row_end>
            <!--模板inp文件里，网格单元编号开始与结束的行-->
            <!--小件一共127105个网格单元，网格单元编号从第188202行开始，第315306行结束-->
            <elem_row_start>188202</elem_row_start>
            <elem_row_end>315306</elem_row_end>
        </inp>
        <mesh>
            <!--各节点的坐标文件-->
            <node_coordinate>node_coordinate_small.csv</node_coordinate>
            <!--各网格单元包含的节点编号-->
            <element_node>element_node_small.csv</element_node>
            <!--各网格单元的质心坐标-->
            <element_centroid>element_centroid_small.csv</element_centroid>
            <!--大数据集各行对应的网格单元的编号-->
            <element_preserve>element_preserve_small.csv</element_preserve>
        </mesh>
        <region>
            <!--各危险区域包含的网格单元编号-->
            <region_elem>region_elem_small.csv</region_elem>
            <!--各应变片测点包含的网格单元编号-->
            <clip>clip_small.csv</clip>
            <!--各应变片测点包含的网格单元编号，及该编号对应大数据集哪一行-->
            <clip_preserve>clip_preserve_small.csv</clip_preserve>
        </region>
        <core>
            <core_grade>small_B_grade_top5.csv</core_grade>
            <core_sort>small_B_sort_top5_R1.csv</core_sort>
        </core>
        <FM>
            <!--可以参与计算的力和力矩的组合-->
            <simu_list>simu_list_small.csv</simu_list>
            <!--抛弃掉的力和力矩的组合-->
            <simu_abandon>simu_abandon_small.csv</simu_abandon>
        </FM>
        <job>
            <!--存放inp和odb的文件夹-->
            <odb_folder>C:/Users/Desktop/ts_0</odb_folder>
            <job_id_start>19001</job_id_start>
            <job_id_end>20000</job_id_end>
            <!--记录不合格的odb-->
            <bug_odb>bug_odb_small_0.txt</bug_odb>
            <!--合格的odb的大小-->
            <odb_KB_down>121000</odb_KB_down>
            <odb_KB_up>121020</odb_KB_up>
            <!--从odb中提取出的大数据集存放的文件夹-->
            <data_folder>C:/dataset/small_1</data_folder>
            <!--从odb中提取出的大数据集文件的命名-->
            <data_file_end>_SEY_small.csv</data_file_end>
        </job>
        <NN_A>
            <!--神经网络所需数据集存放位置-->
            <NN_A_data_folder>C:/dataset/small_data</NN_A_data_folder>
            <!--神经网络所需数据集命名-->
            <NN_A_data_file>small_A_data_1.csv</NN_A_data_file>
        </NN_A>
        <NN_B>
            <!--神经网络所需数据集存放位置-->
            <NN_B_data_folder>C:/dataset/small_data</NN_B_data_folder>
            <!--神经网络所需数据集命名-->
            <NN_B_data_S_file>small_B_data_S_R1_1.csv</NN_B_data_S_file>
            <NN_B_data_E_file>small_B_data_E_R1_1.csv</NN_B_data_E_file>
            <NN_B_data_S_core_top5_file>small_B_data_S_core_top5_R1_1.csv</NN_B_data_S_core_top5_file>
            <NN_B_data_E_core_top5_file>small_B_data_E_core_top5_R1_1.csv</NN_B_data_E_core_top5_file>
        </NN_B>
    </small>
    <!--**********大型零件**********-->
    <large>
        <path_save>C:/projects/make_dataset/files_large/</path_save>
        <inp>
            <inp_template>Job_TMPL_large.inp</inp_template>
            <node_row_start>10</node_row_start>
            <node_row_end>290234</node_row_end>
            <elem_row_start>290236</elem_row_start>
            <elem_row_end>484682</elem_row_end>
        </inp>
        <mesh>
            <node_coordinate>node_coordinate_large.csv</node_coordinate>
            <element_node>element_node_large.csv</element_node>
            <element_centroid>element_centroid_large.csv</element_centroid>
            <element_preserve>element_preserve_large.csv</element_preserve>
        </mesh>
        <region>
            <region_elem>region_elem_large.csv</region_elem>
            <clip>clip_large.csv</clip>
            <clip_preserve>clip_preserve_large.csv</clip_preserve>
        </region>
        <core>
            <core_grade>large_B_grade_top5.csv</core_grade>
            <core_sort>large_B_sort_top5_R1.csv</core_sort>
        </core>
        <FM>
            <simu_list>simu_list_large.csv</simu_list>
            <simu_abandon>simu_abandon_large.csv</simu_abandon>
        </FM>
        <job>
            <odb_folder>C:/Users/Desktop/tl_0</odb_folder>
            <job_id_start>30001</job_id_start>
            <job_id_end>35000</job_id_end>
            <bug_odb>bug_odb_large_1.txt</bug_odb>
            <odb_KB_down>186600</odb_KB_down>
            <odb_KB_up>186620</odb_KB_up>
            <data_folder>C:/dataset/large_0</data_folder>
            <data_file_end>_SEY_large.csv</data_file_end>
        </job>
        <NN_A>
            <!--神经网络所需数据集存放位置-->
            <NN_A_data_folder>C:/dataset/large_data</NN_A_data_folder>
            <!--神经网络所需数据集命名-->
            <NN_A_data_file>large_A_data_1.csv</NN_A_data_file>
        </NN_A>
        <NN_B>
            <!--神经网络所需数据集存放位置-->
            <NN_B_data_folder>C:/dataset/large_data</NN_B_data_folder>
            <!--神经网络所需数据集命名-->
            <NN_B_data_S_file>large_B_data_S_R1_1.csv</NN_B_data_S_file>
            <NN_B_data_E_file>large_B_data_E_R1_1.csv</NN_B_data_E_file>
            <NN_B_data_S_core_top5_file>large_B_data_S_core_top5_R1_1.csv</NN_B_data_S_core_top5_file>
            <NN_B_data_E_core_top5_file>large_B_data_E_core_top5_R1_1.csv</NN_B_data_E_core_top5_file>
        </NN_B>
    </large>

</root>
